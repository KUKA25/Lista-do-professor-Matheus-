
#include <stdio.h>
#include <locale.h>


int main()
{
	setlocale(LC_ALL,"Portuguese");
    int tab = 0;
    printf("\nDigite a tabuada que vocÃª deseja ser exibida: ");
    scanf("%i", &tab);

    for(int x = 1; x<=10; ++x){
       
        printf("%ix%i = %i\n", x, tab, x * tab);
    }

    return 0;
}
